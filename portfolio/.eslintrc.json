{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "quotes": ["error", "single",
         {
            "allowTemplateLiterals": true
         }
      ],
      "indent": ["error", 2],
      "comma-dangle": ["error", "never"],
      "eol-last": ["error", "never"],
      "react/no-children-prop": "off",
      "react/no-unescaped-entities": "off"
    }
}
